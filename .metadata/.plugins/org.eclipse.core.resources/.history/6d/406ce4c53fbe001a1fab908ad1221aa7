package entities;

public class Student {
	public String name;
	public int note1;
	public int note2;
	public int note3;
	
	public double finalGrade() {
		return note1+note2+note3;
	}
	
	public void result() {
		if(finalGrade() >= 60 ) {
			System.out.println("PASS");
		}
		else {
			System.out.println("FAILED");
		}
	}

}
