package entities;

public class Student {
	public String name;
	public float note1;
	public float note2;
	public float note3;
	
	public double finalGrade() {
		return note1+note2+note3;
	}
	
	public void result() {
		if(finalGrade() >= 60 ) {
			System.out.println("PASS");
		}
		else {
			System.out.println("FAILED");
		}
	}
	
	public String toString() {
		return "name: %s"+ name;
	}

}
